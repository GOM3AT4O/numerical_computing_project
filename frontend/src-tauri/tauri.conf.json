{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "numerical_computing_project",
  "version": "0.1.0",
  "identifier": "com.github.tareksaeed0.numerical_computing_project",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/numerical_computing_project/browser"
  },
  "app": {
    "windows": [
      {
        "title": "Numerical Computing Project",
        "width": 500,
        "height": 550
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "externalBin": ["../sidecars/backend"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
